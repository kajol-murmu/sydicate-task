{
  "sqs_handler-role": {
    "predefined_policies": [
      "AWSLambdaBasicExecutionRole"
    ],
    "principal_service": "lambda",
    "custom_policies": [
      {
        "Effect": "Allow",
        "Action": [
          "sqs:ReceiveMessage",
          "sqs:DeleteMessage",
          "sqs:GetQueueAttributes"
        ],
        "Resource": "*"
      }
    ],
    "resource_type": "iam_role",
    "tags": {}
  }
}